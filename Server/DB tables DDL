CREATE TABLE MAINCONV (
  CREATEDBY varchar(64) DEFAULT NULL,
  CREATETS timestamp NULL DEFAULT CURRENT_TIMESTAMP,
  CONVID varchar(20) NOT NULL
);

CREATE TABLE CONVERSATION (
  CONTEXT varchar(512) DEFAULT NULL,
  CREATEDBY varchar(64) DEFAULT NULL,
  CREATETS timestamp NULL DEFAULT CURRENT_TIMESTAMP,
  CONVID varchar(20) NOT NULL
)
